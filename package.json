{
  "name": "@povio/scaffold",
  "version": "1.0.1",
  "author": "Marko Zabreznik <marko.zabreznik@povio.com> (https://zabreznik.net)",
  "scripts": {
    "build": "tsup",
    "cli": "DEBUG_COLORS=1 DEBUG='scaffold:*' node -r @swc-node/register src/cli.ts",
    "test": "DEBUG_COLORS=1 DEBUG='scaffold:*' node --test -r @swc-node/register src/**/*.test.ts",
    "test:watch": "DEBUG_COLORS=1 DEBUG='scaffold:*' node --test --watch -r @swc-node/register src/**/*.test.ts",
    "lint": "eslint \"{src,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,test}/**/*.ts\" --fix"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "bin": "./dist/cli.js",
  "dependencies": {
    "@types/debug": "^4.1.12",
    "@types/interpret": "^1.1.3",
    "@types/rechoir": "^0.6.4",
    "@types/semver": "^7.5.8",
    "@types/yargs": "^17.0.32",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "cosmiconfig": "^9.0.0",
    "debug": "^4.3.4",
    "fast-glob": "^3.3.2",
    "interpret": "^3.1.1",
    "rechoir": "^0.8.0",
    "semver": "^7.6.1",
    "supports-color": "^9.4.0",
    "ts-morph": "^22.0.0",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "yargs": "^17.7.2",
    "zod": "^3.23.7"
  },
  "devDependencies": {
    "@swc-node/register": "^1.9.1",
    "@swc/core": "^1.5.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5"
  },
  "packageManager": "yarn@4.2.1"
}
